
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="runMeTask" class="com.lbl.scheduler.SubmissionTask" />


	<bean name="runMeJob" class="org.springframework.scheduling.quartz.JobDetailFactoryBean">

		<property name="jobClass" value="com.lbl.scheduler.SubmissionJob" />

		<property name="jobDataAsMap">
			<map>
				<entry key="runMeTask" value-ref="runMeTask" />
			</map>
		</property>
		<property name="durability" value="true" />

	</bean>


	<bean id="cronTrigger" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
		<property name="jobDetail" ref="runMeJob" />
		<property name="cronExpression" value="0 20 03 1/1 * ? *" />

	</bean>

	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="jobDetails">
			<list>
				<ref bean="runMeJob" />
			</list>
		</property>

		<property name="triggers">
			<list>
				<ref bean="cronTrigger" />
			</list>
		</property>
	</bean>
	

	<bean id="gmbTask" class="com.lbl.scheduler.GMBSchedulerTask" />


	<bean name="gmbJob" class="org.springframework.scheduling.quartz.JobDetailFactoryBean">

		<property name="jobClass" value="com.lbl.scheduler.GMBSchedulerJob" />

		<property name="jobDataAsMap">
			<map>
				<entry key="runMeTask" value-ref="gmbTask" />
			</map>
		</property>
		<property name="durability" value="true" />

	</bean>

	<bean id="cronGMB" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">

		<property name="jobDetail" ref="gmbJob" />
		<property name="cronExpression" value="0 40 06 1/1 * ? *" />

	</bean>

	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="jobDetails">
			<list>
				<ref bean="gmbJob" />
			</list>
		</property>

		<property name="triggers">
			<list>
				<ref bean="cronGMB" />
			</list>
		</property>
	</bean>
	

	 <bean id="bingTask" class="com.lbl.scheduler.BingSchedulerTask" />


	<bean name="bingJob" class="org.springframework.scheduling.quartz.JobDetailFactoryBean">

		<property name="jobClass" value="com.lbl.scheduler.BingSchedulerJob" />

		<property name="jobDataAsMap">
			<map>
				<entry key="runMeTask" value-ref="bingTask" />
			</map>
		</property>
		<property name="durability" value="true" />

	</bean>



	<bean id="cronBing" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">

		<property name="jobDetail" ref="bingJob" />
		<property name="cronExpression" value="0 21 11 1/1 * ? *" />

	</bean>

	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="jobDetails">
			<list>
				<ref bean="bingJob" />
			</list>
		</property>

		<property name="triggers">
			<list>
				<ref bean="cronBing" />
			</list>
		</property>
	</bean>

	<bean id="sweetIQTask" class="com.lbl.scheduler.SIQSchedulerTask" />

	<bean name="sweetIQJob" class="org.springframework.scheduling.quartz.JobDetailFactoryBean">

		<property name="jobClass" value="com.lbl.scheduler.SIQSchedulerJob" />

		<property name="jobDataAsMap">
			<map>
				<entry key="runMeTask" value-ref="sweetIQTask" />
			</map>
		</property>
		<property name="durability" value="true" />

	</bean>



	<bean id="cronSweetIQ" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">

		<property name="jobDetail" ref="sweetIQJob" />
		<property name="cronExpression" value="	0 58 12 15 1/1 ? *" />

	</bean>

	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="jobDetails">
			<list>
				<ref bean="sweetIQJob" />
			</list>
		</property>

		<property name="triggers">
			<list>
				<ref bean="cronSweetIQ" />
			</list>
		</property>
	</bean> 
	

</beans>